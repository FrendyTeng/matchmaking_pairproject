<%- include("partials/header", { isLogged: true})%>
        
    <h1>People That I Like</h1>
    <hr>
        <table border= 1>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Birth Date</th>
                <th>Gender</th>
            </tr>
            <% for(let i = 0; i < data.length; i++){ %>
            <tr>
                <td><%= data[i].User.firstName %></td>
                <td><%= data[i].User.lastName %></td>
                <td><%= data[i].User.birthDate %></td>
                <td><%= data[i].User.gender %></td>
            </tr>
            <% } %>
        </table>

<%- include("partials/footer") %>